   "source": [
    "import matplotlib.pyplot as plt\n",
    "import numpy as np\n",
    "\n",
    "n = int(input(\"Enter the value of n: \"))\n",
    "x = np.linspace(-2*np.pi,2*np.pi,1000)\n",
    "u = n*x\n",
    "y = np.sin(u)/x\n",
    "plt.plot(y)\n",
    "plt.title(\"sin(nx)/x\")\n",
    "plt.xlabel(\"X-axis\")\n",
    "plt.ylabel(\"Y-axis\")\n",
    "plt.show()"
   ]
